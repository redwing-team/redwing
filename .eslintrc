{
    "env": {
        "browser": true
    },
    "extends": ["airbnb-base"],
    "plugins": ["html"],
    "rules": {
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "import/no-unresolved": 0,
        "import/no-extraneous-dependencies": 0,
        "comma-dangle": 0,
        "no-new": 0,
        "semi": 0,
        "no-param-reassign": 0,
        "linebreak-style": 0,
        "array-callback-return": 0,
        "no-mixed-operators": 0,
        "no-console": 0,
        "no-bitwise": 0,
        "no-plusplus": 0,
        "eol-last": 0,
        "no-confusing-arrow": 0,
        "space-before-function-paren": 0,
        "arrow-parens": 0,
        "prefer-template": 1,
        "no-underscore-dangle": 1,
        "no-alert": 1,
        "no-debugger": 1
    }
}
